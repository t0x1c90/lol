Commands['Go AFK'] = {
    ['Aliases'] = {'afk', 'brb'},
    ['Function'] = function(Args)
        AFK = not AFK
        if (not AFK) then
            return
        end
        spawn(
            function()
                while AFK do
                    wait(0.5)
                    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(AFKMessage, 'All')
                end
            end
        )
    end
}
